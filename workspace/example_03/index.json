{
  "@context": {
    "@vocab": "http://www.example.org#"
  },
  "features": {
    "cindy": {
      "description": "Inference example about dating",
      "inference": {
        "data": ["./cindy_personal_space/*", "reflexives.n3", "symmetric.n3"],
        "query": "./query_all.n3"
      }
    },
    "cindy_proof": {
      "description": "returns proof instead.",
      "inference": {
        "data": ["./cindy_personal_space/*", "reflexives.n3", "symmetric.n3"],
        "query": "./query_all.n3",
        "options": {
          "proof": true
        }
      }
    },
    "cindy_turtle": {
      "description": "forces default content type to be Turtle.",
      "Content-Type": "text/turtle",
      "inference": {
        "data": ["./cindy_personal_space/*", "reflexives.n3", "symmetric.n3"],
        "query": "./query_all.n3"
      }
    },
    "next": "../example_04"
  }
}
